{% extends 'base.html.twig' %}

{% block title %}{{ page.name }}{% endblock %}

{% block body %}
    <article>
        <div class="row">
            <div class="col">
                <section>
                    <h1>{{ page.name }}</h1>
                    {{ page.content | raw }}
                </section>
            </div>
        </div>
    </article>
{% endblock %}